{
    "manifest_version": 3,
    "name": "Extension Filter",
    "description": "A Chrome extension that filters extensions in the Chrome store",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "https://www.googleapis.com/auth/chromewebstore.readonly"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }